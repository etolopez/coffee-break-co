{
  "name": "@coffee-passport/api",
  "version": "1.0.0",
  "description": "Coffee Digital Passport API - NestJS backend",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/main.js",
    "dev": "tsx src/main.ts",
    "start:dev": "tsx --watch src/main.ts",
    "start:debug": "tsx --inspect src/main.ts",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.470.0",
    "@aws-sdk/s3-request-presigner": "^3.470.0",
    "@clickhouse/client": "^0.2.10",
    "@coffee-passport/shared": "file:../../packages/shared",
    "@nestjs/apollo": "^12.2.2",
    "@nestjs/bull": "^10.2.3",
    "@nestjs/common": "^10.4.20",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.4.20",
    "@nestjs/graphql": "^12.2.2",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.4.20",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/terminus": "^10.3.0",
    "@opensearch-project/opensearch": "^2.4.0",
    "@opentelemetry/api": "^1.7.0",
    "@opentelemetry/auto-instrumentations-node": "^0.41.0",
    "@opentelemetry/sdk-node": "^0.48.0",
    "@prisma/client": "^5.22.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^2.1.1",
    "bullmq": "^5.57.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "fastify": "^4.29.1",
    "fastify-compress": "^1.1.0",
    "fastify-helmet": "^7.1.0",
    "fastify-multipart": "^5.3.1",
    "fastify-plugin": "^4.5.1",
    "fastify-rate-limit": "^5.9.0",
    "ioredis": "^5.7.0",
    "jose": "^5.10.0",
    "jsonld": "^8.1.0",
    "passport": "^0.7.0",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.16.3",
    "pg-format": "^1.0.4",
    "pino-std-serializers": "^6.0.0",
    "prisma": "^5.22.0",
    "rdf-validate-shacl": "^0.1.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.3.0",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.5",
    "@types/passport-http-bearer": "^1.0.35",
    "@types/supertest": "^2.0.16",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
